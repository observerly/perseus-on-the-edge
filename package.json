    "drizzle-orm": "^0.25.4",
    "nitropack": "latest",
    "pathe": "^1.1.0",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.17.6",
    "tsx": "^3.12.7"
  }
